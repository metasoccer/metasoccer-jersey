    "ts-loader": "^8.0.17",
    "typedoc": "^0.24.7",
    "typescript": "4.6.4",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.15.0"
